<%- include ../layouts/cabecera.ejs %>
<br>
<br>
<form method="post" action="../actualizar/" enctype="multipart/form-data">

    <input type="text" name="id" value="<%= proyecto.id %> ">

    <div class="form-group">
        <label for="my-input">CODIGO</label>
        <input id="my-input" value="<%= proyecto.codigo %>" class="form-control"  type="text" name="codigo">
    </div>
    <div class="form-group">
        <label for="my-input">CECO</label>
        <input id="my-input" value="<%= proyecto.ceco_id %>" class="form-control" type="text" name="ceco">
    </div>
    <div class="form-group">
        <label for="my-input">CLIENTE</label>
        <input id="my-input" value="<%= proyecto.cliente %>"  class="form-control" type="text" name="cliente">
    </div>
    <div class="form-group">
        <label for="my-input">NOMBRE CORTO</label>
        <input id="my-input" value="<%= proyecto.nomCorto %>"  class="form-control" type="text" name="nombreCorto">
    </div>
    <div class="form-group">
        <label for="my-input">GERENTE</label>
        <input id="my-input" value="<%= proyecto.empleados_id %>" class="form-control" type="text" name="gerente">
    </div>

    <button type="submit" class="btn btn-outline-primary"><i class='bx bx-check-circle bx-md'></i></button>
    <a href="/proyectos" class="btn btn-outline-danger"><i class='bx bx-x-circle bx-md'></i></a>

</form>

<%- include ../layouts/pie.ejs %>